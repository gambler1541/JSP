<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>JSP 예제 expression.jsp</title>
</head>
<body>
	<% int year = 365; %>
	<% out.println("1년은 약 몇 주일까요? <p>"); %>
	<%= year / 7 %>
	<%= " 주 입니다.<br>" %>
	<% out.println(year / 7 + "주 입니다."); %>
</body>
</html>